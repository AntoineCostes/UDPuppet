{
  "name":"Bobine",
  "type":"OSC",
  "path":"Sans Gravité",

  "version":"1.3.2",
  "description":"Contrôle simplifiée d\'une bobine animatronique ",


  "hasInput":true,
  "hasOutput":true,

  "hideDefaultCommands":true,

  "defaults":
  {
    "autoAdd":false,
    "OSC Input":
    {
      "localPort":10002
    },

		"OSC Outputs":
		{
			"OSC Output":
      {
        "local":false,
        "remoteHost":" ",
        "remotePort":9000
      }
    }
  },

  "parameters":
  {
    "Version":
    {
      "type":"String",
      "default":"Gaillac22-1.3.2",
      "readOnly":true
    },
    "Invocation":
    {
      "type":"Trigger"
    },
    "Vitesse":
    {
      "type":"Float",
      "min":-1,
      "max":1,
      "default":0
    },
    "Reset position":
    {
      "type":"Trigger"
    },
    "Position Relative":
    {
      "type":"Float",
      "min":0,
      "max":1,
      "default":0.5,
      "readOnly":true
    },
    "Position Min":
    {
      "type":"Integer",
      "min": -1000,
      "max": 0,
      "default": -10
    },
    "Position Max":
    {
      "type":"Integer",
      "min": 0,
      "max": 1000,
      "default": 10
    },
    "Position Relative Cible":
    {
      "type":"Float",
      "min":0,
      "max":1,
      "default":0.5
    },
    "Rejoindre Position Relative":
    {
      "type":"Trigger"
    },
    "Deplacement Cible":
    {
      "type":"Float",
      "min":-5,
      "max":5,
      "default":0
    },
    "Deplacer":
    {
      "type":"Trigger"
    },
    "Vitesse Max":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "default": 1
    },
    "Acceleration":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "default": 1
    },
    "Stop":
    {
      "type":"Trigger"
    }
  },

  "hideDefaultParameters": ["useHierarchy",  "autoFeedback", "autoAdd", "colorSendMode", "oscInput/localPort", "oscOutputs/oscOutput/local", "oscOutputs/oscOutput/remotePort", "oscOutputs/oscOutput/listenToFeedback"],

  "scripts":
  [
    "bobine.js"
  ],

  "values":
  {
    "Batterie":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "readOnly": true
    },
    "Carte SD Detectee":
    {
      "type":"Boolean",
      "readOnly":true
    },
    "Position Tours":
    {
      "type":"Float",
      "default":0,
      "readOnly":true
    },
    "Position Pas":
    {
      "type":"Integer",
      "default":0,
      "readOnly":true
    }
  },

  "commands":
  {
    "Aller à":
    {
      "menu":"Mouvement",
      "callback":"goTo",
      "parameters":
      {
        "Value":
        {
          "type":"Float",
          "min":0,
          "max":1,
          "default":0.5
        }
      }
    },
    "Bouger de":
    {
      "menu":"Mouvement",
      "callback":"moveTo",
      "parameters":
      {
        "Value":
        {
          "type":"Integer",
          "min":-100,
          "max":100,
          "default":0
        }
      }
    },
    "Vitesse":
    {
      "menu":"Mouvement",
      "callback":"setSpeed",
      "parameters":
      {
        "Value":
        {
          "type":"Float",
          "min":-1.0,
          "max":1.0,
          "default":0
        }
      }
    },
    "Stop":
    {
      "menu":"Mouvement",
      "callback":"stop",
    },
    "Reset Position":
    {
      "menu":"Statut",
      "callback":"resetPosition",
    },
    "Lancer Sequence":
    {
      "menu":"Sequence",
      "callback":"playSequence",
      "parameters":
      {
        "Name":
        {
          "type":"String"
        },
        "FPS":
        {
          "type":"Integer",
          "min":0,
          "max":200,
          "default":30
        }
      }
    },
    "Arrêter Sequence":
    {
      "menu":"Sequence",
      "callback":"stopSequence",
      "parameters":
      {
      }
    }
  }
}

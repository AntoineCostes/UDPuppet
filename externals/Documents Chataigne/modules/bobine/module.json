{
  "name":"Bobine",
  "type":"OSC",
  "path":"Sans Gravité",

  "version":"1.4.0",
  "description":"Contrôle simplifiée d\'une bobine animatronique ",


  "hasInput":true,
  "hasOutput":true,

  "hideDefaultCommands":true,

  "defaults":
  {
    "autoAdd":false,
    "OSC Input":
    {
      "localPort":10002
    },

		"OSC Outputs":
		{
			"OSC Output":
      {
        "local":false,
        "remoteHost":"bobine.local",
        "remotePort":9000
      }
    }
  },

  "parameters":
  {
    "Version":
    {
      "type":"String",
      "default":"Avril2022-1.4.2",
      "readOnly":true
    },
    "IP":
    {
      "type":"String",
      "default":"",
      "readOnly":true
    },
    "Invocation":
    {
      "type":"Trigger"
    },
    "Sequences":
    {
      "type":"Enum"
    },
    "Play":
    {
      "type":"Trigger"
    },
    "Position Min":
    {
      "type":"Integer",
      "min": -1000,
      "max": 0,
      "default": -10
    },
    "Position Max":
    {
      "type":"Integer",
      "min": 0,
      "max": 10,
      "default": 10
    },
    "Reset position":
    {
      "type":"Trigger"
    },
    "Vitesse Max":
    {
      "type":"Float",
      "min": 0,
      "max": 1000,
      "default": 1000
    },
    "Acceleration":
    {
      "type":"Float",
      "min": 0,
      "max": 5000,
      "default": 1000
    },
    "Stop":
    {
      "type":"Trigger"
    },
    "Release":
    {
      "type":"Trigger"
    }
  },

  "hideDefaultParameters": ["useHierarchy",  "autoFeedback", "autoAdd", "colorSendMode", "pass_through", "oscInput/local", "oscOutputs/oscOutput/local", "oscOutputs/oscOutput/remotePort", "oscOutputs/oscOutput/listenToFeedback"],

  "scripts":
  [
    "bobine.js"
  ],

  "values":
  {
    "Batterie":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "readOnly": true
    },
    "Carte SD Detectee":
    {
      "type":"Boolean",
      "readOnly":true
    },
    "Position Tours":
    {
      "type":"Float",
      "default":0,
      "readOnly":true
    },
    "Position Pas":
    {
      "type":"Integer",
      "default":0,
      "readOnly":true
    },
    "Position Relative":
    {
      "type":"Float",
      "min":0,
      "max":1,
      "default":0.5,
      "readOnly":true
    }
  },

  "commands":
  {
    "Modifier couleur":
    {
      "callback":"setColor",
      "parameters":
      {
        "Couleur":
        {
          "type":"Color"
        }
      }
    },
    "Aller à":
    {
      "menu":"Mouvement",
      "callback":"goToRel",
      "parameters":
      {
        "Position relative":
        {
          "type":"Float",
          "min":0,
          "max":1,
          "default":0.5
        }
      }
    },
    "Bouger de":
    {
      "menu":"Mouvement",
      "callback":"moveTo",
      "parameters":
      {
        "Nombre de pas":
        {
          "type":"Integer",
          "min":-10000,
          "max":10000,
          "default":0
        }
      }
    },
    "Vitesse":
    {
      "menu":"Mouvement",
      "callback":"setSpeed",
      "parameters":
      {
        "Nombre de pas/sec":
        {
          "type":"Float",
          "min":-1000.0,
          "max":1000.0,
          "default":0
        }
      }
    },
    "Stop":
    {
      "menu":"Mouvement",
      "callback":"stop",
    },
    "Release":
    {
      "menu":"",
      "callback":"release",
    },
    "Reset Position":
    {
      "callback":"resetPosition",
    },
    "Lancer Sequence":
    {
      "menu":"Sequence",
      "callback":"playSequence",
      "parameters":
      {
        "Name":
        {
          "type":"String"
        },
        "FPS":
        {
          "type":"Integer",
          "min":0,
          "max":200,
          "default":30
        }
      }
    },
    "Arrêter Sequence":
    {
      "menu":"Sequence",
      "callback":"stopSequence",
      "parameters":
      {
      }
    }
  }
}

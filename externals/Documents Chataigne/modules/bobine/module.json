{
  "name":"Bobine",
  "type":"OSC",
  "path":"Sans Gravité",

  "version":"1.0.0",
  "description":"Contrôle simplifiée d\'une bobine animatronique ",


  "hasInput":true,
  "hasOutput":true,

  "hideDefaultCommands":true,

  "defaults":
  {
    "autoAdd":false,
    "OSC Input":
    {
      "localPort":10002
    },

		"OSC Outputs":
		{
			"OSC Output":
      {
        "local":false,
        "remoteHost":" ",
        "remotePort":9000
      }
    }
  },

  "parameters":
  {
    "Invocation":
    {
      "type":"Trigger"
    },
    "Vitesse Max":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "default": 1
    },
    "Acceleration":
    {
      "type":"Float",
      "min": 0,
      "max": 1,
      "default": 1
    },
  "Reset position":
  {
    "type":"Trigger"
  },
  "Position Absolue":
  {
    "type":"Float",
    "default":0,
    "readOnly":true
  },
  "Position Abs en Pas":
  {
    "type":"Integer",
    "default":0,
    "readOnly":true
  },
  "Position Min":
  {
    "type":"Integer",
    "min": -1000,
    "max": 0,
    "default": -10
  },
"Position Max":
{
  "type":"Integer",
  "min": 0,
  "max": 1000,
  "default": 10
},
  "Position Relative":
  {
    "type":"Float",
    "min":0,
    "max":1,
    "default":0.5,
    "readOnly":true
  },
  "Rejoindre Position Cible":
  {
    "type":"Trigger"
  },
  "Deplacement Cible":
  {
    "type":"Trigger"
  },
  "Stop":
  {
    "type":"Trigger"
  }
  },

  "hideDefaultParameters": ["useHierarchy",  "autoFeedback", "autoAdd", "colorSendMode", "oscInput/localPort", "oscOutputs/oscOutput/local", "oscOutputs/oscOutput/remotePort", "oscOutputs/oscOutput/listenToFeedback"],

  "scripts":
  [
    "bobine.js"
  ],

  "values":
  {
    "Position Cible":
    {
      "type":"Float",
      "min":0,
      "max":1,
      "default":0.5
    },
    "Deplacement Cible":
    {
      "type":"Float",
      "min":-5,
      "max":5,
      "default":0
    },
    "Vitesse":
    {
      "type":"Float",
      "min":-0.2,
      "max":0.2,
      "default":0
    }
  },

  "commands":
  {
    "Aller à":
    {
      "menu":"Mouvement",
      "callback":"goTo",
      "parameters":
      {
        "Value":
        {
          "type":"Float",
          "min":0,
          "max":1,
          "default":0.5
        }
      }
    },
    "Bouger de":
    {
      "menu":"Mouvement",
      "callback":"moveTo",
      "parameters":
      {
        "Value":
        {
          "type":"Integer",
          "min":-100,
          "max":100,
          "default":0
        }
      }
    },
    "Vitesse":
    {
      "menu":"Mouvement",
      "callback":"setSpeed",
      "parameters":
      {
        "Value":
        {
          "type":"Float",
          "min":-0.2,
          "max":0.2,
          "default":0
        }
      }
    },
    "Stop":
    {
      "menu":"Mouvement",
      "callback":"stop",
    },
    "Reset Position":
    {
      "menu":"Statut",
      "callback":"resetPosition",
    },
    "Lancer Sequence":
    {
      "menu":"Sequence",
      "callback":"playSequence",
      "parameters":
      {
        "Name":
        {
          "type":"String"
        },
        "FPS":
        {
          "type":"Integer",
          "min":0,
          "max":200,
          "default":30
        }
      }
    },
    "Arrêter Sequence":
    {
      "menu":"Sequence",
      "callback":"stopSequence",
      "parameters":
      {
      }
    }
  }
}

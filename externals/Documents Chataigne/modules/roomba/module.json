{
  "name":"Roomba",
  "type":"OSC",
  "path":"cie AMK",

  "version":"1.0.0",
  "description":"OSC control of Roomba vacuum cleaner with ESP32",


  "hasInput":true,
  "hasOutput":true,

  "hideDefaultCommands":false,

  "defaults":
  {
    "autoAdd":true,
    "booleanSendMode":2,
    "OSC Input":
    {
      "localPort":10005
    },

		"OSC Outputs":
		{
			"OSC Output":
      {
        "local":false,
        "remoteHost":" ",
        "remotePort":9000
      }
    }
  },

  "hideDefaultParameters": ["useHierarchy",  "autoFeedback", "autoAdd", "booleanSendMode", "colorSendMode"],


  "parameters":
  {
    "Invocation":
    {
      "type":"Trigger"
    }
  },

  "scripts":
  [
    "roomba.js"
  ],

  "values":
  {

  },

  "commands":
  {
    "Start Passive Mode":
    {
      "menu":"System",
      "callback":"startPassive",
    },
    "Start Safe Mode":
    {
      "menu":"System",
      "callback":"startSafe",
    },
    "Start Full Mode":
    {
      "menu":"System",
      "callback":"startFull",
    },
    "Wake Up":
    {
      "menu":"System",
      "callback":"wakeUp",
    },
    "Switch Baud Rate":
    {
      "menu":"System",
      "callback":"switchBaudRate",
    },
    "Init after battery change":
    {
      "menu":"System",
      "callback":"init",
    },
    "Move":
    {
      "menu":"Motion",
      "callback":"move",
      "parameters":
      {
        "Direction":
        {
          "type":"Enum",
          "options":{"forwards":"0", "left":"1", "right":"2", "backwards":"3"}
        },
        "Speed":
        {
          "type":"Float",
          "min":0,
          "max":1,
          "default":0.5
        },
      }
    },
    "Wheels":
    {
      "menu":"Motion",
      "callback":"setMotors",
      "parameters":
      {
        "Left":
        {
          "type":"Float",
          "min":-1,
          "max":1,
          "default":0
        },
          "Right":
          {
            "type":"Float",
            "min":-1,
            "max":1,
            "default":0
          }
      }
    },
    "Stop":
    {
      "menu":"Motion",
      "callback":"stop",
    },
    "Display text":
    {
      "menu":"Display",
      "callback":"displayText",
      "parameters":
      {
        "Text":
        {
          "type":"String",
          "default":"Hello world!"
        }
      }
    },
    "Set leds":
    {
      "menu":"Display",
      "callback":"setLeds",
      "parameters":
      {
        "Dock":
        {
          "type":"Boolean"
        },
        "Spot":
        {
          "type":"Boolean"
        },
        "Warning":
        {
          "type":"Boolean"
        },
        "Dirt":
        {
          "type":"Boolean"
        }
      }
    },
    "Set center led":
    {
      "menu":"Display",
      "callback":"setCenterLed",
      "parameters":
      {
        "Hue":
        {
          "type":"Integer",
          "min":0,
          "max":255
        },
        "Brightness":
        {
          "type":"Integer",
          "min":0,
          "max":255
        }
      }
    },
    "Play song":
    {
      "menu":"Sound",
      "callback":"playSong",
      "parameters":
      {
        "Index":
        {
          "type":"Integer",
          "min":0,
          "max":3
        }
      }
    },
    "Play note":
    {
      "menu":"Sound",
      "callback":"playNote",
      "parameters":
      {
        "Pitch":
        {
          "type":"Enum",
          "options":{"C":"0", "C#":"1", "D":"2", "D#":"3", "E":"4", "F":"5", "F#":"6", "G":"7", "G#":"8", "A":"9", "A#":"10", "B":"11"}
        },
        "Octave":
        {
          "type":"Integer",
          "min":1,
          "max":7
        },
        "Duration":
        {
          "type":"Enum",
          "options":{"0.062":"4", "0.125":"8", "0.156":"10", "0.250":"16",
            "0.31":"20", "0.39":"25", "0.469":"30", "0,5":"32",
            "0.625":"40", "0.78":"50", "1":"64",
            "1,172":"75", "1,5":"96", "1,56":"100", "1,98":"127"}

        }
      }
    },
    "Neopixel":
    {
      "callback":"setNeopixel",
      "parameters":
      {
        "Color":
        {
          "type":"Color"
        }
      }
    },
    "Brushes":
    {
      "callback":"setBrushes",
      "parameters":
      {
        "Vacuum":
        {
          "type":"Boolean"
        },
        "Main":
        {
          "type":"Boolean"
        },
        "Side":
        {
          "type":"Boolean"
        }
      }
    }
  }
}
